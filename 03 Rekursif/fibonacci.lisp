; DEFENISI DAN SPESIFIKASI
; Fib : integer ≥ 0 → integer ≥ 0
; { Definisi rekursif fungsi fibonacci : }
; { Fib (n) = sesuai dengan definisi deret fibonacci :
; 			n = 0 : Fib(0) = 0
; 			n= 1 : Fib(1) = 1
; 			n > 1 : Fib(n) = Fib(n-1) + Fib(n-2) }
;----------------------------------------------------------------------------------------------------------------------------------
; REALISASI
; Fib(n) : depend on (n)
; 			n = 0 : 0 {Basis 0}
; 			n = 1 : 1 {Basis 1}
; 			n > 1 : Fib(n-1) + Fib( n - 2) {Rekurens}
;----------------------------------------------------------------------------------------------------------------------------------
; REALISASI LSP
(defun fib (a b n)
	(cond	((= n 1) a)
			((= n 2) b)
			(t (+ (fib a b (- n 1)) (fib a b (- n 2))))
	)
)
;----------------------------------------------------------------------------------------------------------------------------------
; APLIKASI
; (fib a b n)
